{
  "name": "scss-module-roots",
  "version": "0.0.1",
  "authors": [
    "Yoan marchal <marchalyoan@gmail.com>"
  ],
  "description": "add my scss components into scss project",
  "moduleType": "globals",
  "main": [
    "js/main.js",
    "sass/main.scss"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "components-font-awesome": "~4.4.0",
    "headroom.js": "~0.7.0",
    "isotope": "~2.2.2",
    "jquery": "~2.1.4",
    "magnific-popup": "~1.0.0"
  },
  "overrides": {
    "isotope": {
      "main": [
        "./dist/isotope.pkgd.min.js"
      ],
      "dependencies": {
        "get-size": ">=1.1.8 <1.3",
        "matches-selector": ">=1 <2",
        "outlayer": "1.3.x",
        "masonry": "3.2.x"
      }
    },
    "outlayer": {
      "main": [
        "item.js",
        "outlayer.js"
      ]
    }
  }
